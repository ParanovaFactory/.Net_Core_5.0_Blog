<!DOCTYPE html>
<html>

@await Html.PartialAsync("/Views/Partial/Head.cshtml")

<body>
    <!-- Sidenav -->
    <nav class="sidenav navbar navbar-vertical  fixed-left  navbar-expand-xs navbar-light bg-white" id="sidenav-main">
        <div class="scrollbar-inner">
            <!-- Brand -->
            <div class="sidenav-header  align-items-center">
                <a class="navbar-brand" href="javascript:void(0)">
                    <img src="~/webargon/assets/img/brand/blue.png" class="navbar-brand-img" alt="...">
                </a>
            </div>
            
            @await Html.PartialAsync("/Views/Partial/Sidebar.cshtml")

        </div>
    </nav>
    <!-- Main content -->
    <div class="main-content" id="panel">
        <!-- Topnav -->
        @await Html.PartialAsync("/Views/Partial/Navbar.cshtml")
        <!-- Header -->
        <!-- Header -->
        @await Html.PartialAsync("/Views/Partial/Header.cshtml")
        <!-- Page content -->
        <div class="container-fluid mt--6">
            <div class="row">
                <div class="col">
                        @RenderBody()
                </div>
            </div>
            <br />
            <br />
            <!-- Footer -->
            @await Html.PartialAsync("/Views/Partial/Footer.cshtml")
        </div>
    </div>
    <!-- Scripts -->
    @await Html.PartialAsync("/Views/Partial/Script.cshtml")
</body>

</html>
